{
  "name": "vue-flex-waterfall",
  "description": "A horizontal sorting waterfall layout component for Vue.js, realized by flex layout",
  "version": "2.1.0",
  "files": [
    "dist/*.js",
    "dist/*.map",
    "typings"
  ],
  "types": "./typings/index.d.ts",
  "main": "./dist/vue-flex-waterfall.umd.js",
  "module": "./dist/vue-flex-waterfall.es.js",
  "unpkg": "./dist/vue-flex-waterfall.umd.js",
  "jsdelivr": "./dist/vue-flex-waterfall.umd.js",
  "exports": {
    ".": {
      "import": "./dist/vue-flex-waterfall.es.js",
      "require": "./dist/vue-flex-waterfall.umd.js"
    }
  },
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "dev:demo": "cross-env CONFIG_TYPE=demo vite",
    "build:demo": "cross-env CONFIG_TYPE=demo npm run build",
    "preview:demo": "cross-env CONFIG_TYPE=demo vite preview",
    "lint": "eslint . --ext .vue,.js,.ts"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^14.18.22",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vitejs/plugin-vue": "^3.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "eslint-plugin-vue": "^9.3.0",
    "lodash-es": "^4.17.21",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.3",
    "vite-plugin-css-injected-by-js": "^2.0.0",
    "vue": "^3.2.37",
    "vue-tsc": "^0.39.2"
  },
  "author": "Jindai Kirin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tsuk1ko/vue-flex-waterfall.git"
  },
  "bugs": {
    "url": "https://github.com/Tsuk1ko/vue-flex-waterfall/issues"
  },
  "homepage": "https://tsuk1ko.github.io/vue-flex-waterfall/",
  "keywords": [
    "vue",
    "flex",
    "waterfall"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
