{
  "name": "vue-flex-waterfall",
  "description": "A horizontal sorting waterfall layout component for Vue.js, realized by flex layout",
  "version": "1.0.7",
  "files": [
    "dist/*.js",
    "dist/*.map"
  ],
  "main": "./dist/vue-flex-waterfall.umd.js",
  "module": "./dist/vue-flex-waterfall.mjs",
  "unpkg": "./dist/vue-flex-waterfall.umd.js",
  "jsdelivr": "./dist/vue-flex-waterfall.umd.js",
  "exports": {
    ".": {
      "import": "./dist/vue-flex-waterfall.mjs",
      "require": "./dist/vue-flex-waterfall.umd.js"
    }
  },
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "dev:demo": "cross-env CONFIG_TYPE=demo vite",
    "build:demo": "cross-env CONFIG_TYPE=demo npm run build",
    "preview:demo": "cross-env CONFIG_TYPE=demo vite preview",
    "lint": "eslint . --ext .vue,.js,.ts"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.0.1",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vitejs/plugin-vue": "^2.3.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.19.0",
    "eslint-plugin-vue": "^9.1.1",
    "lodash-es": "^4.17.21",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "vite": "^2.9.13",
    "vite-plugin-css-injected-by-js": "^1.5.1",
    "vue": "^3.2.37",
    "vue-tsc": "^0.38.2"
  },
  "author": "Jindai Kirin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tsuk1ko/vue-flex-waterfall.git"
  },
  "bugs": {
    "url": "https://github.com/Tsuk1ko/vue-flex-waterfall/issues"
  },
  "homepage": "https://tsuk1ko.github.io/vue-flex-waterfall/",
  "keywords": [
    "vue",
    "flex",
    "waterfall"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
